{
  "name": "luxury-poker-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r --filter ./apps/** run build",
    "dev": "pnpm -r --parallel --filter ./apps/** run dev",
    "lint": "pnpm -r --filter ./apps/** run lint || true",
    "typecheck": "pnpm -r --filter ./apps/** run typecheck || true",
    "test": "pnpm -r --filter ./apps/** run test || true"
  }
}
