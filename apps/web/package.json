{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc -b && vite build && cp src/sw.ts dist/sw.js || true",
    "preview": "vite preview --host 0.0.0.0 --port 4173",
    "lint": "echo (stub)",
    "typecheck": "tsc -p tsconfig.json --noEmit",
