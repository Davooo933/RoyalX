    "react-dom": "^18.3.1",
    "@poker/ui": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "vitest": "^2.0.5"
  }
}
